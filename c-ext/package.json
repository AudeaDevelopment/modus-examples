{
  "name": "c-extension",
  "version": "0.1.0",
  "main": "src/main.js",
  "scripts": {
    "build": "parcel build src/main.js --no-minify -d src/content/ -o main.js && parcel build src/popup.html --public-url ./ --no-minify -d src/content -o popup.html",
    "watch": "parcel watch src/main.js -d src/content/ -o main.js",
    "watch-pop": "NODE_ENV=production parcel watch src/popup.html --public-url ./ -d src/content -o popup.html",
    "clean": "cd src/ && rm -rf build* && cd ../ && yarn build"
  },
  "author": "Audea Development",
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-regenerator": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@postlight/mercury-parser": "^2.1.0",
    "amazon-cognito-identity-js": "^3.0.12",
    "aws-sdk": "^2.451.0",
    "dotenv": "^8.0.0",
    "node-fetch": "^2.5.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loading": "^2.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.3",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-member-expression-literals": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-property-literals": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "node-sass": "^7.0.0"
  }
